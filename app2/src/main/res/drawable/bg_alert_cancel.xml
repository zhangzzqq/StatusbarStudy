<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android" >    <item android:state_pressed="false">        <shape>            <stroke android:color="@color/dialog_btn_confirm" android:width="1dp"/>            <solid android:color="@color/dialog_btn_cancel"/>            <corners android:radius="6dp"/>        </shape>    </item>    <item android:state_pressed="true">        <shape>            <stroke android:color="@color/dialog_btn_confirm" android:width="1dp"/>            <solid android:color="@color/dialog_btn_cancel_click"/>            <corners android:radius="6dp"/>        </shape>    </item></selector>